{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "accessControlListName": {
      "value": "accessControlListName"
    },
    "location": {
      "value": "eastus"
    },
    "annotation": {
      "value": "annotation"
    },
    "configurationType": {
      "value": "File"
    },
    "aclsUrl": {
      "value": "https://ACL-Storage-URL"
    },
    "defaultAction": {
      "value": "Permit"
    },
    "matchConfigurations": {
      "value": [
        {
          "matchConfigurationName": "example-match",
          "sequenceNumber": 123,
          "ipAddressType": "IPv4",
          "matchConditions": [
            {
              "etherTypes": [
                "0x1"
              ],
              "fragments": [
                "0xff00-0xffff"
              ],
              "ipLengths": [
                "4094-9214"
              ],
              "ttlValues": [
                "23"
              ],
              "dscpMarkings": [
                "32"
              ],
              "portCondition": {
                "flags": [
                  "established"
                ],
                "portType": "SourcePort",
                "layer4Protocol": "TCP",
                "ports": [
                  "1-20"
                ],
                "portGroupNames": [
                  "example-portGroup"
                ]
              },
              "protocolTypes": [
                "TCP"
              ],
              "vlanMatchCondition": {
                "vlans": [
                  "20-30"
                ],
                "innerVlans": [
                  "30"
                ],
                "vlanGroupNames": [
                  "example-vlanGroup"
                ]
              },
              "ipCondition": {
                "type": "SourceIP",
                "prefixType": "Prefix",
                "ipPrefixValues": [
                  "10.20.20.20/12"
                ],
                "ipGroupNames": [
                  "example-ipGroup"
                ]
              }
            }
          ],
          "actions": [
            {
              "type": "Count",
              "counterName": "example-counter"
            }
          ]
        }
      ]
    },
    "dynamicMatchConfigurations": {
      "value": [
        {
          "ipGroups": [
            {
              "name": "example-ipGroup",
              "ipAddressType": "IPv4",
              "ipPrefixes": [
                "10.20.3.1/20"
              ]
            }
          ],
          "vlanGroups": [
            {
              "name": "example-vlanGroup",
              "vlans": [
                "20-30"
              ]
            }
          ],
          "portGroups": [
            {
              "name": "example-portGroup",
              "ports": [
                "100-200"
              ]
            }
          ]
        }
      ]
    }
  }
}